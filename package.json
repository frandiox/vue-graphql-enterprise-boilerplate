{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:back \"dev:front {@}\" --",
    "dev:front": "vue-cli-service serve",
    "dev:back": "cd server && yarn dev",
    "build": "vue-cli-service build --modern",
    "build:ci": "cross-env ANALYZE=true yarn build",
    "test": "yarn unit && yarn e2e",
    "test:ci": "yarn unit --coverage --ci --maxWorkers=4 && yarn e2e",
    "unit": "vue-cli-service test:unit",
    "unit:watch": "yarn unit --watch --notify --notifyMode change",
    "e2e": "vue-cli-service test:e2e --headless",
    "e2e:dev": "vue-cli-service test:e2e --mode=development",
    "lint":
      "eslint --ext .js,.vue --fix . && stylelint --fix \"src/**/*.vue\" \"src/**/*.scss\" && markdownlint docs/*.md *.md && prettier --list-different --write \"**/*.{js,json,css,scss,vue,md}\"",
    "new": "hygen new",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "prisma": "cd server && yarn prisma",
    "playground": "cd server && yarn playground"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/vue-fontawesome": "^0.1.0",
    "auth0-js": "^9.8.0",
    "lodash": "^4.17.0",
    "normalize.css": "^8.0.0",
    "nprogress": "^0.2.0",
    "vue": "^2.5.0",
    "vue-apollo": "^3.0.0-beta.26",
    "vue-meta": "^1.5.0",
    "vue-router": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-e2e-cypress": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.0",
    "@vue/cli-plugin-unit-jest": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-graphql": "^3.0.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-unicorn": "^6.0.0",
    "eslint-plugin-vue": "5.0.0-beta.3",
    "express": "^4.16.0",
    "hygen": "^1.6.0",
    "imagemin-lint-staged": "^0.3.0",
    "jest-fetch-mock": "^1.7.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^8.0.0",
    "markdownlint-cli": "^0.13.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.8.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.0",
    "vue-cli-plugin-apollo": "~0.18.0",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.0",
    "vuepress": "^0.14.0",
    "webpack-bundle-analyzer": "^3.0.0"
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  },
  "workspaces": ["server"]
}
