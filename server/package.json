{
  "name": "backend",
  "version": "0.0.0",
  "scripts": {
    "start":
      "nodemon -e js,json,graphql,gql -x node -r dotenv/config src/index.js",
    "debug":
      "nodemon -e js,json,graphql,gql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql --dotenv .env playground",
    "dev": "npm-run-all --parallel start playground",
    "prisma": "prisma"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.13.1",
    "jsonwebtoken": "8.2.1",
    "prisma-binding": "~2.0.0"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "express-jwt": "^5.3.1",
    "graphql-cli": "~2.16.0",
    "jwks-rsa": "^1.2.1",
    "nodemon": "1.17.3",
    "npm-run-all": "^4.1.2",
    "prisma": "~1.8.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.5.0"
  }
}
